{
    "servers": [
        {
            "name": "uptimekuma.localhost",
            "locations": [
                {
                    "path": "/",
                    "to": {
                        "host": "uptimekuma",
                        "port": "3001",
                        "schema": "http"
                    }
                }
            ]
        },
        {
            "name": "nginx.localhost",
            "locations": [
                {
                    "path": "/",
                    "to": {
                        "host": "nginx",
                        "port": "80",
                        "schema": "http"
                    }
                }
            ]
        }
    ]
}
